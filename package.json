{
  "name": "ollama-html-ui",
  "version": "1.0.0",
  "description": "Ollama HTML UI",
  "type": "module",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "server": "node scripts/server.js",
    "test": "npm run test:all && npm run test:mobile && npm run test:video",
    "test:all": "node --test --test-reporter=spec",
    "test:mobile": "MOBILE=true node --test --test-reporter=spec",
    "test:video": "VIDEO=true node --test --test-reporter=spec",
    "test:only": "node  --test --test-reporter=spec --test-only",
    "format": "prettier --write \"**/*.{js,html,scss}\"",
    "lint-js": "eslint \"**/*.js\" --fix",
    "lint-css": "stylelint \"**/*.scss\" --fix",
    "build": "parcel build index.html"
  },
  "devDependencies": {
    "cssnano": "^6.0",
    "eslint": "^8.56",
    "eslint-config-prettier": "^9.1",
    "eslint-config-standard": "^17.1",
    "eslint-plugin-import": "^2.29",
    "eslint-plugin-n": "^16.6",
    "eslint-plugin-promise": "^6.1",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "prettier": "^3.2.2",
    "sass": "^1.69.6",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0"
  },
  "author": "Christian Hellsten",
  "license": "MIT"
}
